{
  "name": "turborepo-react",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "eslint-config-next": "workspace:*",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "turbo": "1.5.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.12.2"
}
